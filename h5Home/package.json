{
  "name": "nuxt",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "cxz0822 <593319431@qq.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env PATH_TYPE=local nuxt -c env/nuxt.config-dev.js",
    "analyze": "cross-env PATH_TYPE=test nuxt build --analyze -c env/nuxt.config-test.js",
    "build": "cross-env PATH_TYPE=test nuxt build -c env/nuxt.config-test.js ",
    "start": "cross-env PATH_TYPE=test nuxt start -c env/nuxt.config-test.js",
    "build-exp": "cross-env PATH_TYPE=exploitation nuxt build -c env/nuxt.config-exp.js ",
    "start-exp": "cross-env PATH_TYPE=exploitation nuxt start -c env/nuxt.config-exp.js",
    "build-prod": "cross-env PATH_TYPE=online nuxt build -c env/nuxt.config-prod.js",
    "start-prod": "nuxt start -c env/nuxt.config-prod.js",
    "build-ysc": "cross-env PATH_TYPE=ysc nuxt build -c env/nuxt.config-ysc.js",
    "start-ysc": "cross-env PATH_TYPE=ysc nuxt start -c env/nuxt.config-ysc.js",
    "build-test2": "cross-env PATH_TYPE=test2 nuxt build -c env/nuxt.config-test2.js",
    "start-test2": "cross-env PATH_TYPE=test2 nuxt start -c env/nuxt.config-test2.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.4",
    "@nuxtjs/proxy": "^1.3.1",
    "axios": "^0.19.0-beta.1",
    "element-theme": "~2.0.1",
    "element-ui": "~2.4.9",
    "iscroll": "^5.2.0",
    "mint-ui": "~2.2.13",
    "nuxt": "~1.4.4",
    "qs": "^6.5.2"
  },
  "devDependencies": {
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "eslint": "4.9.0",
    "eslint-config-standard": "10.2.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-html": "3.1.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "ftp-deploy": "^2.1.4",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-style-loader": "^4.1.2"
  }
}
